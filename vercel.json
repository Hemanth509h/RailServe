{
  "version": 2,
  "name": "railserve",
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/css/(.*)",
      "dest": "/public/css/$1"
    },
    {
      "src": "/static/js/(.*)",
      "dest": "/public/js/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  },
  "regions": ["iad1"]
}
